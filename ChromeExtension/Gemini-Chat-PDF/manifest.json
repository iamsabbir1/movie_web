{
    "manifest_version": 3,
    "name": "Gemini Chat PDF Maker",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html"
    },
    "description": "Creates a PDF of your Gemini chat conversation.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://gemini.google.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  